generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BuildOrder {
  id          String @id @default(cuid())
  matchUp     String
  style       String @default("Macro")
  author      String @default("Anonymous")
  description String @default("No Description")
  title       String @default("No Title")
  views       Int    @default(0)
  buildSteps  Json[]
}
